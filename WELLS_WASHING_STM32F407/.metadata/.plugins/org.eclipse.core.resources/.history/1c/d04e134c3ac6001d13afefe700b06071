#ifndef INC_UART_H_
#define INC_UART_H_


#include "main.h"

#define UART_BUF_SIZE 512
#define UART_BUF_SIZE 512
typedef struct
{
	uint8_t size;
	uint8_t data[100];
}_uart_frame;

void uart_dma_start(void);
void uart_process(void);
void uart_dma_stop(void);
HAL_StatusTypeDef uart_transmit_block(uint8_t *data,uint16_t size,uint16_t timeout);



#endif /* INC_UART_H_ */
